<!-- <!-- Modal para Agregar Soporte -->
<div class="modal fade" id="agregarSoporteModal" tabindex="-1" aria-labelledby="agregarSoporteModalLabel" aria-hidden="true" data-bs-backdrop="static" data-bs-keyboard="false">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <form id="FormAgregarSoporte">
                <div class="modal-header">
                    <h5 class="modal-title" id="agregarSoporteModalLabel">Agregar Soporte</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <!-- Contenido del modal -->
                    <div class="row">
                        <!-- Campo: Descripción del soporte -->
                        <div class="col-12 mb-3">
                            <div class="form-floating">
                                <input type="text" class="form-control" id="InpAgregarSoporteDescripcion" name="InpAgregarSoporteDescripcion" required placeholder="Descripción del soporte" maxlength="150" autocomplete="off">
                                <label for="InpAgregarSoporteDescripcion">Descripción del soporte</label>
                            </div>
                        </div>
                    </div>
                    <!-- Editor de texto enriquecido -->
                    <!-- Editor de texto personalizado -->
                    <div class="row">
                        <div class="col-12 mb-3">
                            <label for="editorSoporte" class="form-label">Detalles del soporte:</label>
                            <!-- Barra de herramientas del editor -->
                            <div class="editor-toolbar mb-2 border rounded p-2 bg-light">
                                <button type="button" class="btn btn-light btn-sm" data-command="bold" title="Negrita">
                                    <i class="fas fa-bold"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="italic" title="Cursiva">
                                    <i class="fas fa-italic"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="underline" title="Subrayado">
                                    <i class="fas fa-underline"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="justifyLeft" title="Alinear a la izquierda">
                                    <i class="fas fa-align-left"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="justifyCenter" title="Centrar">
                                    <i class="fas fa-align-center"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="justifyRight" title="Alinear a la derecha">
                                    <i class="fas fa-align-right"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="insertUnorderedList" title="Lista con viñetas">
                                    <i class="fas fa-list-ul"></i>
                                </button>
                                <button type="button" class="btn btn-light btn-sm" data-command="insertOrderedList" title="Lista numerada">
                                    <i class="fas fa-list-ol"></i>
                                </button>
                            </div>
                            <!-- Área de edición -->
                            <div id="editorSoporte" contenteditable="true" class="border rounded p-3" style="height: 450px; overflow-y: auto; background-color: #fff; color: black;"></div>
                        </div>
                    </div>
                    <div class="row">
                        <!-- Campo: Cliente -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbCliente" class="form-label">CLIENTE: <span class="text-danger">*</span></label>
                                <div id="cbCliente"></div>
                            </div>
                        </div>
                        <!-- Campo: Sistema -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbSistema" class="form-label">SISTEMA: <span class="text-danger">*</span></label>
                                <div id="cbSistema"></div>
                            </div>
                        </div>
                        <!-- Campo: Modulo -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbModulo" class="form-label">MODULO: <span class="text-danger">*</span></label>
                                <div id="cbModulo"></div>
                            </div>
                        </div>
                        <!-- Campo: Estatus Soporte -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbEstatusSoporte" class="form-label">ESTATUS SOPORTE: <span class="text-danger">*</span></label>
                                <div id="cbEstatusSoporte"></div>
                            </div>
                        </div>
                        <!-- Campo: Tipo Soporte -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbTipoSoporte" class="form-label">TIPO SOPORTE: <span class="text-danger">*</span></label>
                                <div id="cbTipoSoporte"></div>
                            </div>
                        </div>
                        <!-- Campo: Nivel de Servicio -->
                        <div class="col-4 mb-3">
                            <div class="form-group">
                                <label for="cbNivelServicio" class="form-label">NIVEL DE SERVICIO: <span class="text-danger">*</span></label>
                                <div id="cbNivelServicio"></div>
                            </div>
                        </div>
                        <!-- Campo: Cliente que Reporta -->
                        <div class="col-6 mb-3">
                            <div class="form-group">
                                <label for="cbClienteReporte" class="form-label">CLIENTE QUE REPORTA: <span class="text-danger">*</span></label>
                                <div class="w-100" id="cbClienteReporte"></div>
                            </div>
                        </div>
                        <!-- Campo: Usuario interno Involucrado -->
                        <div class="col-6 mb-3">
                            <div class="form-group">
                                <label for="cbUsuarioInvolucrado" class="form-label">USUARIO INVOLUCRADO: <span class="text-danger">*</span></label>
                                <div class="w-100" id="cbUsuarioInvolucrado"></div>
                            </div>
                        </div>
                        <!-- Campo: Archivo -->
                        <div class="col-12 mb-3">
                            <label class="fw-bold mb-2" style="font-size: 1rem;">Subir archivo:</label>
                            <input class="form-control" id="InpArchivoExt" name="InpArchivoExt" type="file"
                                   accept=".png,.jpg,.jpeg,.gif,.pdf,.doc,.docx" />
                        </div>

                        <!-- Previsualización del archivo -->
                        <div class="col-12 mb-3">
                            <div id="previewContainer" class="d-none border rounded p-1 d-flex align-items-center justify-content-between">
                                <div class="d-flex align-items-center">
                                    <div id="previewIcon" class="me-2" style="font-size: 3rem;"></div>
                                    <img id="previewImage" src="" alt="Previsualización" class="img-thumbnail me-2 d-none"
                                         style="max-width: 150px; max-height: 150px; object-fit: contain;">
                                    <div>
                                        <p id="fileName" class="mb-0 fw-bold"></p>
                                        <small id="fileSize" class="text-muted"></small>
                                    </div>
                                </div>
                                <button type="button" class="btn btn-link text-danger p-3" id="removePreview"
                                        aria-label="Eliminar previsualización">
                                    <i class="fas fa-trash-alt"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-primary">Guardar</button>
                </div>
            </form>
        </div>
    </div>
</div>


