            }

            // Asignar eventos a los botones de la barra de herramientas
            $(".editor-toolbar button").on("click", function () {
                const command = $(this).data("command");
                execCommand(command);
            });

            $('#InpArchivoExt').on('change', function (event) {
                const file = event.target.files[0]; // Obtiene el archivo seleccionado.
                if (file) {
                    const fileType = file.type;
                    const fileName = file.name;
                    const fileSize = (file.size / 1024).toFixed(2) + ' KB';
                    const reader = new FileReader();

                    $('#fileName').text(fileName);
                    $('#fileSize').text(fileSize);
                    $('#previewContainer').removeClass('d-none');

                    if (fileType.startsWith('image/')) {
                        // Si es una imagen, mostrarla
                        reader.onload = function (e) {
                            $('#previewImage').attr('src', e.target.result).removeClass('d-none');
                            $('#previewIcon').addClass('d-none'); // Oculta el icono
                        };
                        reader.readAsDataURL(file);
                    } else {
                        // Si es un documento, mostrar un icono
                        $('#previewImage').addClass('d-none'); // Oculta la imagen

                        if (fileName.endsWith('.pdf')) {
                            $('#previewIcon').html('<i class="fas fa-file-pdf text-danger p-3"></i>').removeClass('d-none');
                        } else if (fileName.endsWith('.doc') || fileName.endsWith('.docx')) {
                            $('#previewIcon').html('<i class="fas fa-file-word text-primary p-3"></i>').removeClass('d-none');
                        } else {
                            $('#previewIcon').html('<i class="fas fa-file text-secondary"></i>').removeClass('d-none');
                        }
                    }
                }
            });

            // Evento para eliminar la previsualización del archivo
            $('#removePreview').on('click', function () {
                $('#previewImage').attr('src', '').addClass('d-none'); // Limpia la imagen y la oculta
                $('#previewIcon').html('').addClass('d-none'); // Oculta el icono
                $('#fileName').text(''); // Limpia el nombre del archivo
                $('#fileSize').text(''); // Limpia el tamaño del archivo
                $('#previewContainer').addClass('d-none'); // Oculta el contenedor de previsualización
                $('#InpArchivoExt').val(''); // Limpia el input de archivo
            });

            // Evento para limpiar el formulario al cerrar el modal
            $('#agregarSoporteModal').on('hidden.bs.modal', function () {
                $('#FormAgregarSoporte')[0].reset();
                $('#FormAgregarSoporte').validate().resetForm();
                $('#previewImage').attr('src', '').addClass('d-none');
                $('#previewIcon').html('').addClass('d-none');
                $('#fileName').text('');
                $('#fileSize').text('');
                $('#previewContainer').addClass('d-none');

                // Restablecer el selectedValue del componente Wijmo cbCliente a -1
                if (cbCliente && cbCliente.selectedValue !== undefined) {
                    cbCliente.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbSistema a -1
                if (cbSistema && cbSistema.selectedValue !== undefined) {
                    cbSistema.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbModulo a -1
                if (cbModulo && cbModulo.selectedValue !== undefined) {
                    cbModulo.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbEstatusSoporte a -1
                if (cbEstatusSoporte && cbEstatusSoporte.selectedValue !== undefined) {
                    cbEstatusSoporte.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbTipoSoporte a -1
                if (cbTipoSoporte && cbTipoSoporte.selectedValue !== undefined) {
                    cbTipoSoporte.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbNivelServicio a -1
                if (cbNivelServicio && cbNivelServicio.selectedValue !== undefined) {
                    cbNivelServicio.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbClienteReporte a -1
                if (cbClienteReporte && cbClienteReporte.selectedValue !== undefined) {
                    cbClienteReporte.selectedValue = -1;
                }

                // Restablecer el selectedValue del componente Wijmo cbUsuarioInvolucrado a -1
                if (cbUsuarioInvolucrado && cbUsuarioInvolucrado.selectedValue !== undefined) {
                    cbUsuarioInvolucrado.selectedValue = -1;
                } // Aquí se cierra el último bloque if
            });
        });
    </script>
}
